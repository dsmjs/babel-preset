{
  "name": "@dsmjs/babel-preset",
  "description": "Shareable babel preset for the organization",
  "license": "MIT",
  "version": "0.0.0-semantically-released",
  "main": "lib/index.cjs.js",
  "module": "lib/index.es.js",
  "sideEffects": false,
  "files": [
    "lib/"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": "dsmjs/babel-preset",
  "bugs": "https://github.com/dsmjs/babel-preset/issues",
  "homepage": "https://npm.im/@dsmjs/babel-preset",
  "author": "dsmJS <maintainers@dsmjs.com> (https://dsmjs.com)",
  "scripts": {
    "test": "npm-run-all --print-label --parallel lint:* --parallel test:*",
    "test:unit": "nyc run-s test:unit:base",
    "test:unit:base": "DEBUG=any mocha src/**/*-test.js",
    "coverage:report": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "lint:js": "eslint . --cache",
    "lint:md": "remark . --frail",
    "lint:sensitive": "ban",
    "clean": "rimraf ./lib",
    "prebuild": "run-s clean",
    "build": "npm-run-all --print-label --parallel build:*",
    "build:js": "rollup --config",
    "watch": "run-s 'build:js -- --watch'",
    "prepack": "run-s build",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@babel/register": "^7.6.2",
    "@dsmjs/eslint-config": "1.0.112",
    "@dsmjs/eslint-config-mocha": "1.0.27",
    "@form8ion/commitlint-config": "^1.0.7",
    "@form8ion/remark-lint-preset": "4.0.2",
    "@travi/any": "^2.0.6",
    "ban-sensitive-files": "^1.9.2",
    "chai": "^4.2.0",
    "codecov": "^3.6.1",
    "cz-conventional-changelog": "^3.0.2",
    "husky": "8.0.2",
    "mocha": "^10.0.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.0.0",
    "remark-cli": "^11.0.0",
    "rimraf": "^3.0.0",
    "rollup": "^3.1.0",
    "rollup-plugin-auto-external": "^2.0.0",
    "sinon": "^15.0.0"
  },
  "dependencies": {
    "@form8ion/babel-preset": "1.6.99"
  }
}
